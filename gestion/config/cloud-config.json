{
  "provider": "local",
  "bucket_name": "cmbagent-contexts",
  "region": "us-east-1",
  "sync_enabled": true,
  "backup_enabled": true,
  "cdn_url": null,
  "upload_url": null,
  "rate_limit": {
    "requests_per_minute": 60,
    "delay_between_requests": 1.0
  },
  "providers": {
    "s3": {
      "bucket_name": "cmbagent-contexts",
      "region": "us-east-1",
      "prefix": "contexts/"
    },
    "gcs": {
      "bucket_name": "cmbagent-contexts",
      "prefix": "contexts/"
    },
    "http": {
      "upload_url": "https://api.example.com/upload",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN"
      }
    }
  },
  "domains": {
    "astronomy": {
      "enabled": true,
      "priority": 1
    },
    "finance": {
      "enabled": true,
      "priority": 2
    }
  }
} 